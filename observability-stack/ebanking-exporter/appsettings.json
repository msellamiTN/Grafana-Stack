{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Metrics": {
    "Port": 9200,
    "Path": "/metrics",
    "SimulationInterval": 1000,
    "Metrics": {
      "TransactionsRate": {
        "Min": 1,
        "Max": 10
      },
      "ActiveSessions": {
        "Min": 50,
        "Max": 200
      },
      "RequestDuration": {
        "MinMs": 100,
        "MaxMs": 500
      }
    }
  },
  "AllowedHosts": "*"
}
